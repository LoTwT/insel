{
  "name": "insel",
  "version": "0.0.0",
  "packageManager": "pnpm@8.3.1",
  "description": "insel",
  "main": "index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "play": "node bin/insel.js",
    "preview": "cd build && serve .",
    "lint": "prettier -c .",
    "prettier": "prettier -w .",
    "up": "taze latest -I",
    "prepare": "simple-git-hooks"
  },
  "bin": {
    "insel": "bin/insel.js"
  },
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "cac": "^6.7.14",
    "fs-extra": "^11.1.1",
    "ora": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "lint-staged": "^13.2.1",
    "pnpm": "^8.3.1",
    "prettier": "^2.8.8",
    "rollup": "^3.21.0",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
