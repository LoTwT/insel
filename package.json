{
  "name": "insel",
  "version": "0.0.0",
  "packageManager": "pnpm@8.3.1",
  "description": "insel",
  "main": "index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "play": "node bin/insel.js",
    "preview": "cd build && serve .",
    "lint": "prettier -c .",
    "prettier": "prettier -w .",
    "up": "taze latest -I",
    "prepare": "simple-git-hooks",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "prepare:e2e": "tsx scripts/prepare-e2e.ts"
  },
  "bin": {
    "insel": "bin/insel.js"
  },
  "license": "MIT",
  "dependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "cac": "^6.7.14",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "hast-util-from-html": "^1.0.2",
    "ora": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^3.0.0",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "shiki": "^0.14.2",
    "unified": "^10.1.2",
    "vite": "^4.3.2"
  },
  "devDependencies": {
    "@loadable/component": "^5.15.3",
    "@playwright/test": "^1.32.3",
    "@types/fs-extra": "^11.0.1",
    "@types/hast": "^2.3.4",
    "@types/node": "^18.16.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@vitest/ui": "^0.30.1",
    "execa": "^7.1.1",
    "lint-staged": "^13.2.1",
    "pnpm": "^8.3.1",
    "prettier": "^2.8.8",
    "rollup": "^3.21.0",
    "serve": "^14.2.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "unist-util-visit": "^4.1.2",
    "unplugin-auto-import": "^0.15.3",
    "vitest": "^0.30.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
