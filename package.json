    "up": "taze latest -I"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.16.0",
    "lint-staged": "^13.2.1",
    "pnpm": "^8.3.1",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.9.1",
    "typescript": "^5.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "cac": "^6.7.14"
